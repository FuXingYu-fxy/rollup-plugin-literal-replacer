  "author": "Xingyu Fu",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "crc": "^4.3.2",
    "estree-walker": "^3.0.3",
