  "description": "Rollup plugin for replacing string literals in specific function calls with customizable transformation rules. Ideal for i18n key hashing and localization workflows.",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/FuXingYu-fxy/rollup-plugin-literal-replacer.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "i18n",
    "string-replacement",
    "ast",
    "magic-string",
    "code-transformation"
